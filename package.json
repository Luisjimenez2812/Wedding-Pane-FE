{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc -b && vite build",
    "build:deploy": "npm run build && copy 404.html dist\\404.html",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "router:generate": "npx @tanstack/router-cli generate src/routes",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:all": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,html}\"",
    "code:fix": "npm run format && npm run lint",
    "code:check": "npm run format:check && npm run lint"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.131.8",
    "@tanstack/router-devtools": "^1.131.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tanstack/router-cli": "^1.131.7",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0"
  }
}
